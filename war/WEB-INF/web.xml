<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>MTA_Dorin</servlet-name>
		<servlet-class>com.mta.javacourse.MTA_DorinServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MTA_Dorin</servlet-name>
		<url-pattern>/mta_dorin</url-pattern>
	</servlet-mapping>
	<servlet>
	<servlet-name>NewMassageServlet</servlet-name>
	<servlet-class>com.mta.javacourse.NewMassageServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>NewMassageServlet</servlet-name>
	<url-pattern>/mynewservlet</url-pattern>
	</servlet-mapping>

	<servlet>
	<servlet-name>StockDetailsServlet</servlet-name>
	<servlet-class>com.mta.javacourse.StockDetailsServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>StockDetailsServlet</servlet-name>
	<url-pattern>/stockdetails</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
